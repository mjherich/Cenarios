<%= form_for(@applicant) do |f| %>
  <div class="field">
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
  </div>
  <div class="field">
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
  </div>
  <div class="field">
    <%= f.label :middle_initial %>
    <%= f.text_field :middle_initial %>
  </div>
  <div class="field">
    <%= f.label :street_address %>
    <%= f.text_field :street_address %>
  </div>
  <div class="field">
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>
  <div class="field">
    <%= f.label :zip %>
    <%= f.text_field :zip %>
  </div>
  <div class="field">
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </div>
  <div class="field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :dob %>
    <%= f.text_field :dob %>
  </div>
  <div class="field">
    <%= f.label :social_sec %>
    <%= f.text_field :social_sec %>
  </div>
  <div class="field">
    <%= f.label :position %>
    <%= f.text_field :position %>
  </div>
  <div class="field">
    <%= f.label :citizen %>
    <%= f.text_field :citizen %>
  </div>
  <div class="field">
    <%= f.label :worked_for %>
    <%= f.text_field :worked_for %>
  </div>
  <div class="field">
    <%= f.label :worked_for_when %>
    <%= f.text_field :worked_for_when %>
  </div>
  <div class="field">
    <%= f.label :felony %>
    <%= f.text_field :felony %>
  </div>
  <div class="field">
    <%= f.label :felony_explanation %>
    <%= f.text_field :felony_explanation %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
